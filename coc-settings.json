{
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "filetypes": ["go"]
    }
  },
  "suggest.noselect": false,
  "coc.preferences.diagnostic.displayByAle": true,
  "suggest.floatEnable": true,
  "rust-analyzer.server.path": "$HOME/.cargo/bin/rust-analyzer"
}
